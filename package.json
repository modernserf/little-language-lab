{
  "name": "zebu",
  "version": "0.0.3",
  "description": "little languages with tagged template literals",
  "keywords": [
    "parse",
    "parser",
    "language",
    "compiler"
  ],
  "author": "Justin Falcone <justin@justinfalcone.com>",
  "license": "ISC",
  "engines": {
    "node": ">=10"
  },
  "main": "main.build.js",
  "module": "module.build.mjs",
  "files": [
    "main.build.js",
    "module.build.mjs"
  ],
  "scripts": {
    "lint": "eslint ./src --ext js,mjs",
    "test": "tsc && node dist/test.js",
    "build:cjs": "rollup src/index.mjs -o main.build.js -f cjs",
    "build:es": "rollup src/index.mjs -o module.build.mjs -f es",
    "build": "npm run build:cjs && npm run build:es",
    "watch:test": "watch 'npm test' ./src"
  },
  "devDependencies": {
    "@types/moo": "^0.5.3",
    "eslint": "^7.10.0",
    "mathjs": "^5.4.0",
    "prop-types": "^15.6.2",
    "rollup": "^0.67.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "moo": "^0.5.1"
  }
}
