{
  "name": "zebu",
  "version": "0.0.3",
  "description": "little languages with tagged template literals",
  "keywords": [
    "parse",
    "parser",
    "language",
    "compiler"
  ],
  "author": "Justin Falcone <justin@justinfalcone.com>",
  "license": "ISC",
  "engines": {
    "node": ">=10"
  },
  "main": "main.build.js",
  "module": "module.build.mjs",
  "files": [
    "main.build.js",
    "module.build.mjs"
  ],
  "scripts": {
    "lint": "eslint ./src --ext ts",
    "test": "jest",
    "build:cjs": "rollup src/index.mjs -o main.build.js -f cjs",
    "build:es": "rollup src/index.mjs -o module.build.mjs -f es",
    "build": "npm run build:cjs && npm run build:es",
    "watch:test": "watch 'npm test' ./src"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.10.0",
    "jest": "^26.4.2",
    "mathjs": "^5.4.0",
    "prettier": "^2.1.2",
    "prop-types": "^15.6.2",
    "rollup": "^0.67.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {}
}
